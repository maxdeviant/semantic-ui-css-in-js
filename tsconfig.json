{
  "compilerOptions": {
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "alwaysStrict": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "lib/",
    "rootDir": "src/",
    "strictNullChecks": true,
    "target": "es5"
  },
  "exclude": [
    "node_modules/",
    "lib/",
    "index.d.ts"
  ]
}
